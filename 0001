print("Hello there! What is your name?")
name = input("My name is " )

gender = input("My gender is ")

if gender == "male" or gender =="Male":
    suffix = "Mr."

print("Hello " + suffix  + name )

age = input("How old are you " + suffix + name )
if age >= "18":
    print("Come on In")
else : print("Get out of here kiddo")

